{
  "name": "maoxian-web-clipper",
  "version": "0.1.72",
  "description": "A web extension to clip information from web page. Save it to your local machine to avoid information invalidation. Not bored registration, Not charged.",
  "main": "",
  "scripts": {
    "dev-chromium": "web-ext run --target chromium --source-dir dist/extension/maoxian-web-clipper",
    "dev-firefox": "web-ext run --target firefox-desktop --source-dir dist/extension/maoxian-web-clipper",
    "dev-chromium-lazy": "web-ext run --no-reload --target chromium --source-dir dist/extension/maoxian-web-clipper",
    "dev-firefox-lazy": "web-ext run --no-reload --target firefox-desktop --source-dir dist/extension/maoxian-web-clipper",
    "build-chrome": "cross-env NODE_ENV=production MX_PLATFORM=chrome MX_PLATFORM_ID=$MX_CHROME_ID webpack --mode=production --progress",
    "build-firefox": "cross-env NODE_ENV=production MX_PLATFORM=firefox MX_PLATFORM_ID=$MX_FIREFOX_ID webpack --mode=production --progress",
    "build-all": "npm run build-chrome && npm run build-firefox",
    "build-test": "cross-env NODE_ENV=production webpack --mode=production --progress",
    "watch": "cross-env NODE_ENV=development webpack --mode=development --watch --progress",
    "test": "mocha --recursive --require esm"
  },
  "author": "Mika",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "cross-env": "^7.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "zip-webpack-plugin": "^3.0.0",
    "remove-files-webpack-plugin": "^1.4.0",
    "esm": "^3.2.25",
    "jsdom": "^15.2.1",
    "mocha": "^7.1.0",
    "sinon-chrome": "^3.0.1"
  },
  "dependencies": {
    "webextension-polyfill": "^0.6.0",
    "blueimp-md5": "^2.12.0",
    "roddeh-i18n": "^1.2.0",
    "strip-css-comments": "^4.1.0",
    "css-selector-generator": "^2.1.0",
    "turndown": "^6.0.0",
    "turndown-plugin-gfm": "^1.0.2",
    "awesomplete": "^1.1.5",
    "pikaday": "^1.8.0"
  }
}
